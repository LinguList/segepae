 keywords = {Christentum // christianity;Forschungsergebnisse // research results;Geistliche // clergy;institutioneller Missbrauch // institutional abuse;Nordamerika // North America;Religion // religion;sexueller Kindesmissbrauch // child sexual abuse;Täter*in // offender;USA // USA},
 pages = {195--206},
