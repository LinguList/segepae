 journal = {Journal of Adolescence},
 doi = {10.1016/j.adolescence.2017.01.006}
}


@article{2377,
 abstract = {Adolescents with intellectual disabilities are known to engage in various sexual behavior problems or sexual offending behaviors. This article provides a review of important aspects of risk assessment within the context of a broader, more comprehensive and holistic assessment of these individuals. Pertinent risk and sexual interest assessment tools are identified along with their strengths and limitations. Issues that are often unattended to are addressed, including consideration of the behavioral implications of the young person's diagnosis and level of cognitive functioning, need for sexual knowledge and sexual interest assessment, and issues related to making a mental health diagnosis. Recommendations for future research are also offered.},
 author = {Blasingame, Gerry D.},
