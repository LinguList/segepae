 doi = {10.1080/14681811003666523}
}


@article{991,
 abstract = {To give a tentative response to the questions dramatized by the playwright Tony Kushner (1993), whose two-part play spans the beginning years of the AIDS pandemic, we proceed by way of Eros. This power of love signifies without guarantee, can attach to anything, is easily lost, and is made urgent with our great questions. Desire transmits to the other its mysteries, fissures, madness, frailties, and vulnerabilities, carried on with the question ‘how will I love?’ Without our libido, we erotic creatures would have no reason to think, to fantasize, to dream, to fall in love with people and ideas, and to reach out to others. These creative events of curiosity compose our sexuality and passion, expressed at first through the condensation of object relations and then surprised by our choice of object. We cannot predict whether our choice will be homosexual, heterosexual, or bisexual. And frankly, desire does not care for these categories. Nor can we say what these choices or our sexual preferences will mean over the course of our lives. But we can say that Eros manages to gather all that we want with all that we worry about losing. Indeed, feelings of love that attach to an object call forth anxieties over loss of love. In this sense, Eros is the great unifier: its purpose is to bring faraway things together, a tonic to the destructive capacity of hatred meant to tear things apart and to render the world as worthless and without meaning. And this erotic capacity to bind and to attempt greater unities for the purpose of bringing together disparate pieces of experience and so, to repair what fragments our world, is the basis for how we think. On this view, Eros, a design of sexuality, is the beginning of education as well as an announcement of our potential.},
 author = {Britzman, Deborah P.},
 year = {2010},
 title = {Teachers and Eros},
 keywords = {Intimität // intimacy;Lehrer*innen // teachers;Sexuelle Bildung // sexuality education},
 pages = {325--330},
 volume = {10},
 number = {3},
 issn = {1468-1811},
 journal = {Sex Education},
 doi = {10.1080/14681811.2010.491627}
}


