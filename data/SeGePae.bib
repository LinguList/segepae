}


@article{1614,
 abstract = {The current study examined children and families who presented for treatment through Project SAFE (Sexual Abuse Family Education) following childhood sexual abuse. Pretreatment assessment data were used to develop clusters of participants with significantly differing presentation of symptom outcome following abuse. Four clusters were discovered: (a) a Highly Distressed group, whose members had clinically elevated scores on all self- and parent-report measures; (b) a Problem Behaviors group, whose members had scores within the normal range for self-report measures and elevated scores on all parent-report measures; (c) a Subclinical group, whose participants had scores below the mean and below cutoff scores for all self- and parent-report measures; and (d) a Self-reported Distress group, whose members had elevated scores on self-report measures and scores below clinical cutoffs for all parent-report measures.},
