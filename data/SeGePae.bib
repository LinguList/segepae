}


@article{2191,
 abstract = {Youth between the ages of 13 and 25 are the fastest growing age group among HIV-infected individuals in the United States (U.S.) (Centers for Disease Control and Prevention CDC, 2016a, Centers for Disease Control and Prevention CDC, 2016b; Kaiser Family Foundation, 2012; Schnall, Travers, Rojas, {\&} Carballo-Diéguez, 2014). Black youth (15–24) often bear the burden of HIV as they comprised 60{\%} of all new HIV infections in 2012, compared to Hispanics (20{\%}) and Whites (20{\%}) (Centers for Disease Control and Prevention CDC, 2016b; Schnall, Rojas {\&} Travers, 2015). Given increasing rates of HIV infection among youth populations, especially Blacks, identifying effective prevention efforts is critical (AVERT, 2016; Obermeyer {\&} Osborn, 2007).

Given Black youth are disproportionately affected by HIV, testing for HIV is important to both prevention and treatment efforts. Past research that has examined factors associated with HIV testing, suggests that gender may be the most salient factor associated with HIV testing behaviors, wherein females are more likely to test for HIV than males (Adebayo {\&} Gonzalez-Guarda, 2017; Arrington-Sanders {\&} Ellen, 2008; Balaji et al., 2012; Decker et al., 2015; Inungu et al., 2011; Longmore, Johnson, Manning, {\&} Giordano, 2013). Studies that have examined racial differences in HIV testing report that Black youth, in comparison with other racial-ethnic groups, are more likely to be tested (Adebayo {\&} Gonzalez-Guarda, 2017; Balaji et al., 2012; Decker et al., 2015; Hall, Walker, Shah, {\&} Belle, 2012; Rakhmanina et al., 2014). However, the reasons Black youth test for HIV are unclear. Regarding age, studies have reported that older youth (18–24) are more likely to get tested for HIV than younger youth (15–17) (Arrington-Sanders {\&} Ellen, 2008; Decker et al., 2015; Schrager, Wong, Weiss, {\&} Kipke, 2011; Sumartojo et al., 2008). Moreover, interesting associations have been found between socioeconomic status and HIV testing. Prior studies have found that lower socioeconomic status was associated with the increased likelihood of being tested (Inungu et al., 2011; Johns, Bauermeister, {\&} Zimmerman, 2010; Sumartojo et al., 2008). Other studies have found, low education (Sumartojo et al., 2008) and no education was associated with the decrease in odds of being tested for HIV (Swenson et al., 2010). Understanding how different social factors prevent Black youth from receiving an HIV test can contribute to the development of interventions that promote HIV testing.

Insofar as risk factors are concerned, there is evidence that perception is important; in that, the perception of risk is found to be both a barrier to and a facilitator of HIV testing. (Champion, Harlin, {\&} Collins, 2013; Moyer, Silvestre, Lombardi, {\&} Taylor, 2007; Schnall et al., 2015). Risky sexual behaviors have been operationalized in the literature as the of number of sexual partners or increased sexual promiscuity, enhanced substance use, low condom use, history of sexual abuse, early sexual debut, and with composite measures of sexual behaviors (Arrington-Sanders {\&} Ellen, 2008; Balaji et al., 2012; Coeytaux et al., n.d; Locke {\&} Newcomb, 2008). However, most of this evidence is based on studies that have compared Black youth to others. Very little is known about individual differences among Black youth. Thus, there is a gap in our knowledge about those who are more resilient with respect to managing their sexual behaviors, including getting HIV testing on a regular basis and as needed.

Although limited, research identifies sexual health conversations as one potential HIV prevention strategy for youth populations. Current and past research shows that youth who communicate with their parents and peers about sexual health topics, such as condom use, HIV, and sexually transmitted infections (STI), are more likely to use condoms and limit casual sexual encounters (Crosby et al., 2002; Guilamo-Ramos et al., 2011; Murry et al., 2011; Noar, Carlyle, {\&} Cole, 2006; Hicks, McRee, {\&} Eisenberg, 2013; Widman, Choukas-Bradley, Helms, Golin, {\&} Prinstein, 2014). Studies have also found sexual health conversations to serve as an effective prevention strategy with racial/ethnic minority youth, particularly young Black men. For instance, Santa Maria, Markham, Engebretson, Baumler, and McCurdy' (2014) mixed method study that explored the dynamics of sexual health conversations between single Black mothers (n = 23) and their sons, aged 11 to 14 (n = 25), found that mothers and their sons were both comfortable and uncomfortable with discussing sexual health topics. Nevertheless, most young men expressed that their mothers were approachable, and some reported communicating with their mother about sexual health topics prior to engaging in their first sexual experience.

While studies find sexual health conversations to increase safe decisions surrounding sexual intercourse among youth populations, including racial/ethnic minority youth, this knowledge base is limited. Less is known about the relationship between sexual health conversations and HIV testing, a prevention strategy the Center for Disease Control (CDC) recommends for all individuals between the ages of 13–65 (Centers for Disease Control and Prevention CDC, 2016c). A focus on Black youth aged 13–25 is also important because recent evidence suggests that there is a need to identify effective HIV prevention and education efforts for this population (AVERT, 2016; Centers for Disease Control and Prevention CDC, 2016d; Longmore et al., 2013; Rodriguez, 2014). Furthermore, an understanding of the gender differences surrounding sexual health conversations and HIV testing is also important, as girls between the ages of 17 and 26 are less likely to discuss STI prevention with their partners in comparison to males (Hutchinson,1998, Hicks et al., 2013).},
 author = {Boyd, Donte and Lea, Charles H. and Gilbert, Keon L. and Butler-Barnes, Sheretta T.},
 year = {2018},
 title = {Sexual health conversations: Predicting the odds of HIV testing among black youth and young adults},
 keywords = {Behandlung // treatment;Ethnizität // ethnicity;Gesundheit // health;Gesundheitswesen // public health;Jugend // youth;Nordamerika // North America;Prävalenz // prevalence;Prävention // prevention;Schwarz // Black;Sexuell übertragbare Infektionen // sexually transmitted infections;sexuelle Gesundheit // sexual health;Survey // survey;USA // USA},
 pages = {134--140},
 volume = {90},
 issn = {01907409},
 journal = {Children and Youth Services Review},
 doi = {10.1016/j.childyouth.2018.05.025}
}


@article{2192,
 abstract = {This study explored the link between four types of peer victimization (i.e., school bullying, cyber bullying, physical dating violence, and sexual dating violence) and suicide attempts using a nationally representative survey of high school students (N = 11,341). More specifically, this study examined gender differences in direct and indirect associations between different types of peer victimization, depression, and attempted suicide in a comprehensive path model. Results demonstrated that the effect size of “victim of school bullying” on suicide attempts among female students was greater than it was among male students, but the effect size of “victim of sexual dating violence” on suicide attempts among male students was greater than among female students. In addition, depression partially mediated the relationship between each type of peer victimization and suicide attempts for female students and male students. The indirect effects of “victim of school bullying” and “victim of sexual dating violence” on suicide attempts through the mediation of depression were greater among female students than they were among male students.},
 author = {Kim, Youn Kyoung and Yang, Mi-Youn and Barthelemy, Juan J. and Lofaso, Blaine M.},
 year = {2018},
 title = {A binary gender analysis to bullying, dating violence, and attempted suicide: The disproportionate effect of depression and psychological harm},
 keywords = {Folgen // long-term effects;Forschungsergebnisse // research results;Geschlecht // gender;Gewalt // violence;Gewalt in Beziehungen // dating violence;Jugend // youth;jugendliche Täter*innen // adolescent sex offender;Mobbing // bullying;Nordamerika // North America;Peergewalt // peer violence;Schule // school;sexuelle Gewalt // sexual violence;USA // USA},
 pages = {141--148},
 volume = {90},
 issn = {01907409},
 journal = {Children and Youth Services Review},
 doi = {10.1016/j.childyouth.2018.05.028}
}


@article{2193,
 abstract = {This paper draws upon data from two separate studies to consider sexual health outcomes for young people in state care in Wales. Cross sectional analysis of the School Health Research Network Survey (2015) examined sexual health behaviours and outcomes of young people in foster care compared to those living in private households. The findings are contextualised with qualitative data (collected 2016) from social care professionals about their practice experiences of supporting sexual health development and seeking to prevent early pregnancy for young people in state care. Findings suggest that young people in state care in Wales experience poorer sexual health outcomes compared to those not in care. Insights from professionals suggest that whilst the state care system has the potential to identify and respond to young people's needs, its effectiveness can be compromised by issues of resources, value-based judgements related to the age at which young people could and should be sexually active, and individual choice and determinism. It is suggested that statutory monitoring of sexual health related outcomes is required, together with a review of policy and practice ‘with’, rather than ‘for’, young people in state care. Finally it is argued that increased attention on the quality as well as the quantity of relationships young people in state care have with carers and professionals may provide a simple but powerful policy approach in continued efforts to secure parity in respect of sexual health and development.},
 author = {Roberts, Louise and Long, Sara Jayne and Young, Honor and Hewitt, Gillian and Murphy, Simon and Moore, Graham F.},
 year = {2018},
 title = {Sexual health outcomes for young people in state care: Cross-sectional analysis of a national survey and views of social care professionals in Wales},
 keywords = {Europa // Europe;Forschungsergebnisse // research results;Großbritannien // Great Britain;Kinder- und Jugendhilfe // child welfare system;Pflegefamilie // foster care;qualitative Interviews // qualitative interviews;Schwangerschaft // pregnancy;sexuelle Gesundheit // sexual health;Statistiken // statistics},
 pages = {281--288},
 volume = {89},
 issn = {01907409},
 journal = {Children and Youth Services Review},
 doi = {10.1016/j.childyouth.2018.04.044}
}


@article{2194,
 abstract = {Human trafficking victimization is frequently associated with child abuse or neglect and out-of-home placements. This article presents the largest study to date of human trafficking allegations within a child welfare context, and one of the few to compare children with allegations of human trafficking to others in the child welfare population. It uses state administrative data to examine 4413 allegations of sex and labor trafficking involving 3420 children. These children were more than twice as likely as others to have experienced prior maltreatment. Among children with prior child welfare experience, those with trafficking allegations were twice as likely to have experienced out-of-home placements, >5 times as likely to have experienced congregate care, and >10 times as likely to have run away from placements. Although these data cannot be interpreted as representing the true prevalence of human trafficking within the child welfare population, they expand our understanding of known victims, with associated implications for research, practice and policy.},
 author = {Gibbs, Deborah A. and Henninger, Alana M. and Tueller, Stephen J. and Kluckman, Marianne N.},
 year = {2018},
 title = {Human trafficking and the child welfare population in Florida},
 keywords = {Forschungsergebnisse // research results;Gewalt // violence;Kinder // children;Kinder- und Jugendhilfe // child welfare system;Kindesmissbrauch // child abuse;Menschenhandel // human trafficking;Nordamerika // North America;Statistiken // statistics;USA // USA},
 pages = {1--10},
 volume = {88},
