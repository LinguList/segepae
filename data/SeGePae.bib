 journal = {Journal of Curriculum Studies}
}


@article{699,
 abstract = {The usage of mobile phones and the internet by young people has increased rapidly in the past decade, approaching saturation by middle childhood in developed countries. Besides many benefits, online content, contact or conduct can be associated with risk of harm; most research has examined whether aggressive or sexual harms result from this. We examine the nature and prevalence of such risks, and evaluate the evidence regarding the factors that increase or protect against harm resulting from such risks, so as to inform the academic and practitioner knowledge base. We also identify the conceptual and methodological challenges encountered in this relatively new body of research, and highlight the pressing research gaps.},
 author = {Livingstone, Sonia M. and Smith, Peter K.},
 year = {2014},
 title = {Annual Research Review: Harms experienced by child users of online and mobile technologies: The nature, prevalence and management of sexual and aggressive risks in the digital age},
 keywords = {Europa // Europe;Internet;Kinder // children;Mobbing // bullying;Pornografie // pornography;Risikofaktoren // risk factors;Schutz // protection;Survey;Übersicht // review},
