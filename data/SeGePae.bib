 journal = {Child Abuse Review},
 doi = {10.1002/car.2449}
}


@article{2039,
 abstract = {This toolkit is a valuable resource for Northamptonshire Safeguarding Children Board and its safeguarding agencies in that it collates key research, practice tools and protocols in order to increase awareness of child sexual exploitation (CSE), its early warning signs and appropriate responses to identifying potential cases of CSE for safeguarding professionals and other relevant agencies. Although it has been written specifically for Northamptonshire Local Safeguarding Children Board agencies, this toolkit has wider application across different local safeguarding children boards (LSCBs) as it lends itself to local adaption. It is clearly and coherently laid out and contains the key information needed to increase recognition and understanding of CSE across the safeguarding agencies responsible for working with, and recognising, this potentially vulnerable group of children and young people. Additionally, it would be a useful tool in single- and multiagency CSE training for practitioners.},
 author = {Dodsworth, Jane},
 year = {2017},
 title = {The Northamptonshire Tackling Child Sexual Exploitation Toolkit: A Toolkit to Raise Awareness and Share Strategies to Ensure Early Help is Provided to Children and Young People at Risk of Child Sexual Exploitation by Northamptonshire Safeguarding Children},
