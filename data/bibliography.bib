}


@article{Boustani.2017,
 abstract = {Among girls in foster care, 48{\%} become pregnant at least once by age 19 (Dworsky {\&} Courtney, 2010). Youth in or at-risk for foster care (YFC) report limited knowledge about, access to, and use of condoms; ambivalent attitudes towards teen parenting; and participation in other risky behaviors. For the current study, we adapted and supplemented an evidence-based sexual health program called SiHLE, using a systematic adaptation framework (ADAPT-ITT, Wingood {\&} DiClemete, 2008), to address the unique and targeted needs of youth living in a temporary shelter due to lack of housing. Youth in this study were either in foster care and awaiting placement, or having serious family problems and were at-risk of entering the foster care system.},
