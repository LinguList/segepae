@article{Zafar.2013,
 abstract = {The Childhood Sexual Abuse Stereotypes Scale was developed to assess stereotypes of adult survivors of childhood sexual abuse. Scale items were derived from two studies that elicited cultural and personal beliefs about, and emotions experienced towards adult childhood sexual abuse survivors among university undergraduates. Two scales, Emotions and Characteristics, were developed and administered online to 182 participants. Exploratory factor analysis produced 7 factors, 4 for the Emotions Scale and 3 for the Characteristics Scale. Study 2 replicated this factor structure using confirmatory factor analysis (N = 457). Reliability and validity analyses suggest that the Childhood Sexual Abuse Stereotypes Scale has satisfactory psychometric properties. The Childhood Sexual Abuse Stereotypes Scale can be used to examine stereotypes of legal and health care professionals likely to work with survivors as well as in stereotyping research.},
 author = {Zafar, Sadia and Ross, Erin C.},
 year = {2013},
 title = {Perceptions of childhood sexual abuse survivors: Development and initial validation of a new scale to measure stereotypes of adult survivors of childhood sexual abuse},
 keywords = {Attitude;Erwachsene;Factor Analysis, Statistical;Female;Humans;Kinder;Male;Psychometrics/instrumentation/methods;Reproducibility of Results;sexueller Kindesmissbrauch;Social Perception;Stereotyping;Surveys and Questionnaires/standards;Survivors/psychology;Young Adult},
 pages = {358--378},
