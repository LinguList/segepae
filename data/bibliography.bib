@article{Noonan.2011,
 abstract = {This article arose from an Australian project designed to develop educational and training material in relation to lesbian, gay, bi-sexual and transgender (LGBT) people with disability. The project was supported by the Queensland Association of Healthy Communities (QAHC) and the Queensland Centre for Intellectual and Developmental Disability (QCIDD). A resource was developed and its aim is to create greater awareness and understanding within the community that LGBT people with intellectual disability exist and to provide education and training to disability organizations. We aim to present the complexity of issues which prevent LGBT people with intellectual disability from living full lives and having opportunities for sexual expression.},
