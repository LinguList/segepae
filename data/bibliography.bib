 issn = {0004-0002},
 journal = {Archives of Sexual Behavior}
}


@book{Drobac.2016,
 abstract = {{\dq}When it comes to sexual relationships, adolescents pose a particular problem. Few teenagers possess all of the emotional and intellectual tools needed to navigate these threats, including the all too real advances made by supervisors, teachers, and mentors. The author explores the shockingly common problem of maturing adolescents who are harassed and exploited by adults in their lives. Reviewing the neuroscience and psychosocial evidence of adolescent development, she explains why teens are so vulnerable to adult harassers. Even today, in an age of increasing public awareness, criminal and civil law regarding the sexual abuse of minors remains tragically inept and irregular from state to state. Drobac uses six recent cases of teens suffering sexual harassment to illuminate the flaws and contradictions of this system, skillfully showing how our current laws fail to protect youths, and offering an array of imaginative legal reforms that could achieve increased justice for adolescent victims of sexual coercion{\dq}--Book jacket.},
 author = {Drobac, Jennifer Ann},
 year = {2016},
 title = {Sexual exploitation of teenagers: Adolescent development, discrimination, and consent law},
 keywords = {Gesetzlich / legal;Jugendliche und junge Erwachsene;Konsens // consent;Policies;sexuelle Bel{\"a}stigung // sexual harassment;Sexuelle Entwicklung // sexual development;sexuelle Gewalt // sexual violence},
 address = {Chicago and London},
