@article{Fredlund.2013,
 abstract = {Lifetime experience of selling sex among adolescents was investigated together with sociodemographic correlates, parent-child relationship, and the existence of people to confide in. Changes over time regarding the selling of sex were investigated through a comparison of data from 2004 and 2009. This study was carried out using 3,498 adolescents from a representative sample of Swedish high school students with a mean age 18.3 years. Of these adolescents, 1.5{\%} stated that they had given sexual services for reimbursement and both male and female buyers existed. The adolescents who had sold sex had a poorer parent-child relationship during childhood and had fewer people to confide in about problems and worries. Changes over time were found especially regarding the Internet as a contact source and also immigrant background.},
